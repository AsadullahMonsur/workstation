<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    card_view:cardElevation="8dp"
    card_view:cardUseCompatPadding="true"
    android:layout_gravity="center_horizontal">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal|center_vertical">

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardCornerRadius="10dp"
                android:layout_centerInParent="true"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <ImageView
                    android:id="@+id/j_b_full_deal_card_view_profile_img"
                    android:layout_width="450dp"
                    android:layout_height="300dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="4dp"
                    android:background="@color/TextColor2"/>

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_rent_id"
                    android:layout_width="450dp"
                    android:layout_height="40dp"
                    android:text="@string/loading"
                    android:textSize="20sp"
                    android:textColor="@color/White"
                    android:textAlignment="center"
                    android:layout_marginTop="10dp"
                    android:background="@color/TextColor"/>
            </com.google.android.material.card.MaterialCardView>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section2"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:layout_centerInParent="true"
            android:gravity="center_horizontal"
            android:layout_below="@+id/j_b_full_deal_card_view_section1">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_rent_info"
                    android:layout_width="200dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="20dp"
                    android:background="@color/White"
                    android:fontFamily="cursive"
                    android:text="Rent Info"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="23sp"
                    android:textStyle="italic|bold"/>

                <RelativeLayout
                    android:layout_width="450dp"
                    android:layout_height="2dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:background="@color/Red"
                    android:layout_marginTop="39dp"/>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section3"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_below="@+id/j_b_full_deal_card_view_section2">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="90dp"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical">

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_location1"
                        android:layout_width="150dp"
                        android:layout_height="130dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/location_hint"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_location2"
                        android:layout_width="300dp"
                        android:layout_height="130dp"
                        android:layout_toEndOf="@+id/j_b_full_deal_card_view_location1"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/loading"
                        android:textAlignment="textStart"
                        android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />
                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section4"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section3">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="90dp"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_amount_1"
                        android:layout_width="150dp"
                        android:layout_height="130dp"
                        android:gravity="center_vertical|center_horizontal"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/amount_hint"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_amount_2"
                        android:layout_width="300dp"
                        android:layout_height="130dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:layout_toRightOf="@+id/j_b_full_deal_card_view_amount_1"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/zero_amount"
                        android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />
                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section5"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section4">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="90dp"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal">

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_contact_1"
                        android:layout_width="200dp"
                        android:layout_height="130dp"
                        android:gravity="center_vertical|center_horizontal"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/contact_number_hint"
                        android:textAlignment="center"
                        android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_contact_2"
                        android:layout_width="250dp"
                        android:layout_height="150dp"
                        android:gravity="center_vertical|center_horizontal"
                        android:layout_toRightOf="@+id/j_b_full_deal_card_view_contact_1"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/loading"
                       android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />
                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section6"
            android:layout_width="match_parent"
            android:layout_height="65dp"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section5">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_description"
                    android:layout_width="200dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="20dp"
                    android:background="@color/White"
                    android:fontFamily="cursive"
                    android:text="@string/description_hint"
                    android:textAlignment="textStart"
                    android:textAllCaps="false"
                    android:textColor="@color/colorPrimaryDark"
                    android:textSize="23sp"
                    android:textStyle="italic|bold"/>

                <RelativeLayout
                    android:layout_width="450dp"
                    android:layout_height="2dp"
                    android:layout_marginLeft="5dp"
                    android:layout_marginRight="5dp"
                    android:background="@color/Red"
                    android:layout_marginTop="39dp"
                    android:layout_gravity="center_horizontal"/>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/j_b_full_deal_card_view_section6">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:id="@+id/j_b_full_deal_card_view_description_tv"
                        android:layout_width="440dp"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal|center_vertical"
                        android:background="@color/White"
                        android:fontFamily="serif-monospace"
                        android:text="@string/loading"
                        android:textAllCaps="false"
                        android:textSize="19dp"
                        android:textStyle="bold" />

                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section7">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardCornerRadius="10dp"
                card_view:cardElevation="6dp"
                card_view:cardUseCompatPadding="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:layout_gravity="center_horizontal">

                    <ImageView
                        android:id="@+id/j_b_full_deal_card_view_photos"
                        android:layout_width="440dp"
                        android:layout_height="350dp"
                        android:layout_centerInParent="true"
                        android:src="@mipmap/home_main_2" />

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_photo_btn_next"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="320dp"
                        android:layout_marginLeft="390dp"
                        android:clickable="true"
                        android:backgroundTint="@color/black"
                        card_view:srcCompat="@drawable/next_photo" />


                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_photo_btn_previous"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="320dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:backgroundTint="@color/black"
                        card_view:srcCompat="@drawable/previous_photo"/>
                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section8">

           <com.google.android.material.card.MaterialCardView
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               card_view:cardCornerRadius="10dp"
               card_view:cardElevation="6dp"
               card_view:cardUseCompatPadding="true">

               <RelativeLayout
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:layout_margin="8dp"
                   android:layout_gravity="center_horizontal|center_vertical">

               <Button
                  android:id="@+id/j_b_full_deal_card_view_bottom_btn1"
                  android:layout_width="150dp"
                  android:layout_height="30dp"
                  android:bottomRightRadius="30dp"
                  android:bottomLeftRadius="30dp"
                  android:topLeftRadius="30dp"
                  android:topRightRadius="30dp"
                  android:text="Edit"
                  android:textAlignment="center"
                  android:background="@color/Blue3"/>

               <Button
                  android:id="@+id/j_b_full_deal_card_view_bottom_btn2"
                  android:layout_width="150dp"
                  android:layout_height="30dp"
                  android:layout_toRightOf="@+id/j_b_full_deal_card_view_bottom_btn1"
                  android:bottomRightRadius="10dp"
                  android:bottomLeftRadius="10dp"
                  android:topLeftRadius="10dp"
                  android:topRightRadius="10dp"
                  android:text="Cancel Deal"
                  android:textAlignment="center"
                  android:background="@color/TextColor2"/>

               <Button
                 android:id="@+id/j_b_full_deal_card_view_bottom_btn3"
                 android:layout_width="150dp"
                 android:layout_height="30dp"
                 android:text="Active"
                 android:textAlignment="center"
                 android:background="@color/Green4"
                 android:layout_toRightOf="@id/j_b_full_deal_card_view_bottom_btn2"
                 android:bottomRightRadius="10dp"
                 android:bottomLeftRadius="10dp"
                 android:topLeftRadius="10dp"
                 android:topRightRadius="10dp"/>
              </RelativeLayout>
           </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

  <RelativeLayout
     android:id="@+id/j_b_full_deal_card_view_section10"
     android:layout_width="match_parent"
     android:layout_height="wrap_content"
     android:layout_centerInParent="true"
     android:layout_below="@+id/j_b_full_deal_card_view_section9">

     <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="*"
         android:layout_centerInParent="true"
        android:padding="8dp">

        <TableRow>
           <com.google.android.material.card.MaterialCardView
               android:layout_width="150dp"
               android:layout_height="100dp"
               card_view:cardElevation="0dp"
               card_view:cardUseCompatPadding="true"
               android:background="@color/White"
               android:layout_column="1">

               <TextView
                   android:layout_width="match_parent"
                   android:layout_height="match_parent"
                   android:layout_gravity="center_vertical|center_horizontal"
                   android:gravity="center_vertical|center_horizontal"
                   android:text="@string/general_information"
                   android:textAllCaps="false"
                   android:textColor="@color/ButtonColor"
                   android:textSize="20sp"/>

           </com.google.android.material.card.MaterialCardView>
        </TableRow>

        <TableRow>
            <com.google.android.material.card.MaterialCardView
                android:layout_width="150dp"
                android:layout_height="100dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true"
                android:backgroundTint="@color/Green3"
                android:layout_column="0">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/publish_date"
                    android:textSize="17sp"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/Pink1"
                    />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="150dp"
                android:layout_height="100dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true"
                android:backgroundTint="@color/White"
                android:layout_column="1">

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_publish_date_tv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/unknown_date"
                    android:textSize="17sp"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/Pink1"
                    />

            </com.google.android.material.card.MaterialCardView>
            <com.google.android.material.card.MaterialCardView
                android:layout_width="150dp"
                android:layout_height="100dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true"
                android:backgroundTint="@color/White"
                android:layout_column="2">

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_access_publisher_tv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/available_for_rent"
                    android:textSize="17sp"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/Pink1"
                    />
            </com.google.android.material.card.MaterialCardView>
        </TableRow>

        <TableRow>
            <com.google.android.material.card.MaterialCardView
                android:layout_width="150dp"
                android:layout_height="100dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true"
                android:backgroundTint="@color/Green3"
                android:layout_column="0">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/rent_date"
                    android:textSize="17sp"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/Pink1"
                    />

            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="150dp"
                android:layout_height="100dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true"
                android:backgroundTint="@color/White"
                android:layout_column="1">

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_rent_date_tv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/unknown_date"
                    android:textSize="17sp"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/Pink1"
                    />
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="150dp"
                android:layout_height="100dp"
                card_view:cardElevation="5dp"
                card_view:cardUseCompatPadding="true"
                android:backgroundTint="@color/White"
                android:layout_column="2">

                <TextView
                    android:id="@+id/j_b_full_deal_card_view_access_renter_tv"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="@string/not_rented_yet"
                    android:textSize="17sp"
                    android:textAllCaps="false"
                    android:textAlignment="center"
                    android:textColor="@color/Pink1"
                    />
            </com.google.android.material.card.MaterialCardView>
        </TableRow>

     </TableLayout>
  </RelativeLayout>

    <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section10">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardElevation="0dp"
                card_view:cardUseCompatPadding="true"
                android:layout_centerInParent="true">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center_horizontal">

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_rate_star_btn_1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:backgroundTint="@color/White"
                        card_view:srcCompat="@drawable/rate_star_empty"/>

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_rate_star_btn_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@id/j_b_full_deal_card_view_rate_star_btn_1"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:backgroundTint="@color/White"
                        card_view:srcCompat="@drawable/rate_star_empty"/>

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_rate_star_btn_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_toRightOf="@id/j_b_full_deal_card_view_rate_star_btn_2"
                        android:clickable="true"
                        android:backgroundTint="@color/White"
                        card_view:srcCompat="@drawable/rate_star_empty"/>

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_rate_star_btn_4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_toRightOf="@id/j_b_full_deal_card_view_rate_star_btn_3"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:backgroundTint="@color/White"
                        card_view:srcCompat="@drawable/rate_star_empty"/>

                    <com.google.android.material.floatingactionbutton.FloatingActionButton
                        android:id="@+id/j_b_full_deal_card_view_rate_star_btn_5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:layout_toRightOf="@id/j_b_full_deal_card_view_rate_star_btn_4"
                        android:clickable="true"
                        android:backgroundTint="@color/White"
                        card_view:srcCompat="@drawable/rate_star_empty"/>

                </RelativeLayout>
            </com.google.android.material.card.MaterialCardView>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/j_b_full_deal_card_view_section12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_below="@+id/j_b_full_deal_card_view_section11">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                card_view:cardElevation="0dp"
                card_view:cardUseCompatPadding="true"
