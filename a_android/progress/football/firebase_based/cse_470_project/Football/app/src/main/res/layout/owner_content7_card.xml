<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="520dp"
        app:cardCornerRadius="20dp"
        android:elevation="16dp"
        app:cardUseCompatPadding="true"
        android:backgroundTint="@color/Blue4">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <RelativeLayout
                android:id="@+id/owner_content7_port_00_01"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:background="@color/pink">

                <TextView
                    android:id="@+id/owner_content7_port_00_01_tv"
                    android:layout_width="match_parent"
                    android:text="@string/id"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor2"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_00_02"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_00_01"
                android:background="@color/white">

                <TextView
                    android:id="@+id/owner_content7_port_00_02_tv"
                    android:layout_width="match_parent"
                    android:text="@string/id"
                    android:textAlignment="center"
                    android:textStyle="italic"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
            android:id="@+id/owner_content7_port_01"
            android:layout_width="match_parent"
            android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_00_02">

            <TextView
                android:id="@+id/owner_content7_port_01_tv"
                android:layout_width="match_parent"
                android:text="@string/name"
                android:textAlignment="center"
                android:textStyle="bold"
                android:textAllCaps="false"
                android:textSize="21sp"
                android:textColor="@color/TextColor2"
                android:gravity="center"
                android:layout_height="match_parent"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/owner_content7_port_02"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_below="@id/owner_content7_port_01"
            android:background="@color/white">

                <TextView
                    android:id="@+id/owner_content7_port_02_tv"
                    android:layout_width="match_parent"
                    android:text="@string/name"
                    android:textAlignment="center"
                    android:textStyle="italic"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_03"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_02">

                <TextView
                    android:id="@+id/owner_content7_port_03_tv"
                    android:layout_width="match_parent"
                    android:text="@string/country"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor2"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_04"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_03"
                android:background="@color/white">

                <TextView
                    android:id="@+id/owner_content7_port_04_tv"
                    android:layout_width="match_parent"
                    android:text="@string/country"
                    android:textAlignment="center"
                    android:textStyle="italic"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_05"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_04">

                <TextView
                    android:id="@+id/owner_content7_port_05_tv"
                    android:layout_width="match_parent"
                    android:text="@string/age"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor2"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_06"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_05"
                android:background="@color/white">

                <TextView
                    android:id="@+id/owner_content7_port_06_tv"
                    android:layout_width="match_parent"
                    android:text="@string/age"
                    android:textAlignment="center"
                    android:textStyle="italic"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_07"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_06">

                <TextView
                    android:id="@+id/owner_content7_port_07_tv"
                    android:layout_width="match_parent"
                    android:text="@string/position"
                    android:textAlignment="center"
                    android:textStyle="bold"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor2"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/owner_content7_port_08"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@id/owner_content7_port_07"
                android:background="@color/white">

                <TextView
                    android:id="@+id/owner_content7_port_08_tv"
                    android:layout_width="match_parent"
                    android:text="@string/position"
                    android:textAlignment="center"
                    android:textStyle="italic"
                    android:textAllCaps="false"
                    android:textSize="21sp"
                    android:textColor="@color/TextColor"
                    android:gravity="center"
                    android:layout_height="match_parent"/>
            </RelativeLayout>
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>
</RelativeLayout>