        <activity android:name=".Sign_Up_DB"/>
        <activity android:name=".Log_In_Page_1_DB"/>
        <activity android:name=".Log_In_Page_2_DB"/>
        <activity android:name=".Owner_Home"/>
        <activity android:name=".Manager_Home"/>
        <activity android:name=".Player_Home"/>
        <activity android:name=".Viewer_Home"/>
        <activity android:name=".Income_DB"/>
        <activity android:name=".Set_Matters_DB"/>
        <activity android:name=".Bring_Personal_Info_DB"/>
        <activity android:name=".Bring_Managers_Players_DB"/>
